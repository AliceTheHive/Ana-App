{
  "name": "ana-conversation-studio",
  "version": "0.1.3",
  "main": "electron.js",
  "author": "Team Ana",
  "description": "The Ana app, contains Ana studio, user management, analytics and chatbot publishing",
  "homepage": "http://ana.chat",
  "license": "GNU-GPLv3",
  "build": {
    "appId": "com.ana.studio",
    "productName": "Ana",
    "directories": {
      "output": "../release",
      "app": ".",
      "buildResources": "../build"
    },
    "mac": {
      "icon": "favicon.icns",
      "category": "public.app-category.graphics-design"
    },
    "win": {
      "icon": "favicon.ico"
    },
    "linux": {
      "category": "Graphics"
    },
    "electronVersion": "1.8.2-beta.3",
    "publish": {
      "provider": "generic",
      "url": "https://studio.ana.chat/${os}-${arch}"
    }
  },
  "dependencies": {
    "electron-updater": "^2.16.3"
  }
}
